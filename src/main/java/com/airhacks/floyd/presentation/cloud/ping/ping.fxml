<AnchorPane id="AnchorPane" minHeight="180.0" prefHeight="347.0" prefWidth="525.0" styleClass="airpad" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.airhacks.floyd.presentation.cloud.ping.PingPresenter">
    <children>
        <BarChart fx:id="memoryChart" horizontalGridLinesVisible="false" layoutX="14.0" layoutY="62.0" prefHeight="150.0" prefWidth="162.0" title="Heap">
