                <CategoryAxis fx:id="memoryCategoryAxis" side="BOTTOM" tickLabelGap="10.0" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="memoryNumberAxis" autoRanging="false" side="LEFT" tickLabelGap="10.0" tickMarkVisible="false" />
